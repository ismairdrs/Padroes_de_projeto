      "source": [
        "class Singleton:\n",
        "\n",
        "    def __new__(cls):\n",
        "        if not hasattr(cls, 'instance'):\n",
        "            cls.instance = super(Singleton, cls).__new__(cls)\n",
        "        return cls.instance\n"
      ],
      "execution_count": null,
      "outputs": []
    },
